<div class="blue-footer container">
  <div class="row">

    <div class="col-md-6">
      <%= link_to image_tag('tuftsuniversity.png', :alt => 'Go to tufts.edu'), "http://www.tufts.edu" %>
    </div>

   
    
    <div class="row pull-right col-md-4" id="copyright">
      <span> &copy; <%= Date.today.year %> Tufts University </span>
<% if false %>
      <div class="version">
      Version <span title="<%= GIT_SHA %>"><%= BRANCH %> updated <%= LAST_DEPLOYED %></span>
      </div>
<% end %>
    </div>    
  </div>
</div>
